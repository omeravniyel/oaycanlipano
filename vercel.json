{
    "crons": [
        {
            "path": "/api/cron-reminder",
            "schedule": "0 17 * * 0"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/super-admin",
            "destination": "/super-admin.html"
        },
        {
            "source": "/([^/]+)/admin",
            "destination": "/admin.html"
        },
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}